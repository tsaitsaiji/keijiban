<h1>hello</h1>


<div class="button-container">
    <%= link_to "新しい投稿",new_post_path,class: "link-button"%>
</div>
<ul>
    <% @posts.each do |postt|%>
        <li>
         <h2><%=postt.title %></h2>
         <p><%=postt.content %></p>
         <span>
         <small><%= postt.created_at %></small>
         </span>

         <div class="edit-delete-wrapper">
         <%= link_to "編集",edit_post_path(postt),class: "link-button"%>
          <%= button_to "削除",post_path(postt),method: :delete,class:"link-delete-button" %>
         </div>
        </li>
    <% end %>
</ul>